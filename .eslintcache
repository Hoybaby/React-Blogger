ile\r\n                console.log(data);\r\n                setData(data);\r\n                setisLoading(false);\r\n                setError(null);\r\n            })\r\n            .catch(err => {\r\n                // console.log(err.message);\r\n                setError(err.message);\r\n                setisLoading(false);\r\n            })\r\n    }, [url]);\r\n\r\n\r\n    return { data, isLoading, error}\r\n}\r\n\r\nexport default useFetch;\r\n",{"ruleId":"34","replacedBy":"35"},{"ruleId":"36","replacedBy":"37"},{"ruleId":"38","severity":1,"message":"39","line":1,"column":17,"nodeType":"40","messageId":"41","endLine":1,"endColumn":25},{"ruleId":"38","severity":1,"message":"42","line":1,"column":27,"nodeType":"40","messageId":"41","endLine":1,"endColumn":36},{"ruleId":"38","severity":1,"message":"43","line":1,"column":8,"nodeType":"40","messageId":"41","endLine":1,"endColumn":13},"no-native-reassign",["44"],"no-negated-in-lhs",["45"],"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","'useEffect' is defined but never used.","'React' is defined but never used.","no-global-assign","no-unsafe-negation"]